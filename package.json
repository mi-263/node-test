    "bcrypt": "^5.0.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "form-data": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "progress-stream": "^2.0.0",
    "rxjs": "^7.2.0",
    "swagger-jsdoc": "^6.1.0",
